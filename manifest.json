{
  "manifest_version": 3,
  "name": "Tab Manager Pro",
  "version": "1.3",
  "description": "Organize and save tabs into groups for easy management and productivity.",
  "permissions": ["tabs", "storage", "scripting", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "content/icons/icon16.png",
      "48": "content/icons/icon48.png",
      "128": "content/icons/icon128.png"
    },
    "default_title": "Open Tab Manager Pro"
  },
  "options_page": "options.html",
  "icons": {
    "16": "content/icons/icon16.png",
    "48": "content/icons/icon48.png",
    "128": "content/icons/icon128.png"
  }
}

